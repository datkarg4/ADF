{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "Smart-policy-system"
		},
		"ADLSGEN2_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ADLSGEN2'"
		},
		"archival_path_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'archival_path'"
		},
		"landing_path_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'landing_path'"
		},
		"srcfiles_path_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'srcfiles_path'"
		},
		"ADLSGEN2_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://policydataproject.dfs.core.windows.net/"
		},
		"AzureDataLakeStorage480_properties_privateLinkResourceId": {
			"type": "string",
			"defaultValue": "/subscriptions/a8c0200c-b805-47ea-bde3-5923054a28ac/resourceGroups/missionazure_hrushi/providers/Microsoft.Storage/storageAccounts/policydataproject"
		},
		"AzureDataLakeStorage480_properties_groupId": {
			"type": "string",
			"defaultValue": "dfs"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "Smart-policy-system_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ADLSGEN2_accountKey": {
						"value": "[parameters('ADLSGEN2_accountKey')]"
					},
					"archival_path_connectionString": {
						"value": "[parameters('archival_path_connectionString')]"
					},
					"landing_path_connectionString": {
						"value": "[parameters('landing_path_connectionString')]"
					},
					"srcfiles_path_connectionString": {
						"value": "[parameters('srcfiles_path_connectionString')]"
					},
					"AzureDataLakeStorage480_properties_privateLinkResourceId": {
						"value": "[parameters('AzureDataLakeStorage480_properties_privateLinkResourceId')]"
					},
					"AzureDataLakeStorage480_properties_groupId": {
						"value": "[parameters('AzureDataLakeStorage480_properties_groupId')]"
					},
					"ADLSGEN2_properties_typeProperties_url": {
						"value": "[parameters('ADLSGEN2_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}